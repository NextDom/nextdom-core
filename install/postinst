#!/usr/bin/env bash
set -e

#################################################################################################
################################### NextDom Installation from deb ###############################
################################### Script to remove in the futur ###############################
###################################    .deb package should call   ###############################
################################### # scripts/postinst.sh instead ###############################
#################################################################################################

CURRENT_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" >/dev/null 2>&1 && pwd)"
