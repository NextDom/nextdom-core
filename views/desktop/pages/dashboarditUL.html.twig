{% for object in objectList %}
    <li class="">
        <a href="" data-id="{{ object.getId()}}" data-name="{{ object.getName()}}" >{{ object.getDisplay('icon', '<i class="fas fa-tag"></i>')|raw}}{{ object.getName()}}</a>
        {% if object.getChilds() is not empty %}
            <ul class="nav flex-column">
                {% include "/desktop/pages/dashboarditUL.html.twig" with {'objectList': object.getChilds()} %}
            </ul>
        {% endif %}

    </li>
{% endfor %}