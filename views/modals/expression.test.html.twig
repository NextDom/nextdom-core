<section class="content">
    <div class="box">
        <div class="box-header with-border">
            <h3 class="box-title"><i class="fas fa-question-circle"></i>{{ 'Expression à tester' }}</h3>
        </div>
        <div class="box-body">
            <div class="input-group">
                <input class="form-control" id="in_testExpression">
                <span class="input-group-btn">
                    <a type="button" class="btn btn-default" id="bt_searchInfoCmd" data-toggle="tooltip" title="" data-original-title="{{ 'Rechercher une commande' }}"><i class="fas fa-list-alt"></i></a>
                    <a type="button" class="btn btn-default"  id="bt_searchScenario" data-toggle="tooltip" title="" data-original-title="{{ 'Rechercher un scenario' }}"><i class="fas fa-film"></i></a>
                    <a type="button" class="btn btn-default"  id="bt_searchEqLogic" data-toggle="tooltip" title="" data-original-title="{{ 'Rechercher d\'un équipement' }}"><i class="fas fa-cube"></i></a>
                    <a class="btn btn-warning" id="bt_executeExpressionOk"><i class="fas fa-bolt"></i><span>{{ 'Exécuter' }}</span></a>
                </span>
            </div>
        </div>
    </div>
</section>

<section class="content">
    <div class="box">
        <div class="box-header with-border">
            <h3 class="box-title"><i class="fas fa-poll-h"></i>{{ 'Résultat' }}</h3>
        </div>
        <div class="box-body">
            <div id="div_expressionTestResult"></div>
        </div>
    </div>
</section>

<script src="/public/js/modals/expression.test.js"></script>
